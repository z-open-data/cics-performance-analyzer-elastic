{
  "frequency": "1m",
  "sync": {
    "time": {
      "field": "Collection Time",
      "delay": "60s"
    }
  },
  "source": {
    "index": [
      "cicspa_storage*",
      "cicspa_dsa*"
    ],
    "query": {
      "match_all": {}
